@model List<UserForm.Models.Users>

    <div>
        <table class="table" cellpadding="0" cellspacing="0">
            <tr>
                <th align="center" scope="col">First Name</th>
                <th align="center" scope="col">Last Name</th>
                <th align="center" scope="col">Number</th>
                <th></th>
            </tr>
            @foreach (var item in Model) {
                <tr>
                    <td>@item.FName</td>
                    <td>@item.LName</td>
                    <td>@item.Number</td>
                    <td>
                        @Html.ActionLink(
                        "X",
                        "Delete",
                        "User",
                        new { item.id })
                    </td>
                </tr>
            }
        </table>
    </div>
